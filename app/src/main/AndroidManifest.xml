<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.drsk.progserega.inspectionsheet">

    <application
        android:name=".InspectionSheetApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.SelectTypeLine" />
        <activity android:name=".activities.SearchObject" />
        <activity android:name=".activities.searchBySpRes" />
        <activity android:name=".activities.createInspection" />
        <activity android:name=".activities.CreateDefect" />
        <activity android:name=".activities.FoundedDefects" />
        <activity android:name=".activities.EndInspection" />
        <activity android:name=".activities.InspectTower"></activity>
    </application>

</manifest>